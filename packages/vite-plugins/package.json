{
    "name": "@fizzlab.io/vite-plugins",
    "version": "1.7.0",
    "author": "FizzLab, LLC",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "keywords": [
        "vite",
        "shopify",
        "vite-plugin"
    ],
    "scripts": {
        "dev": "pnpm watch",
        "build": "tsup src/index.ts --dts --format cjs,esm --clean --publicDir",
        "watch": "tsup src/index.ts --dts --format cjs,esm --watch --publicDir",
        "test": "vitest run --mode development"
    },
    "devDependencies": {
        "@fizzlab.io/tsconfig": "workspace:*",
        "prettier": "3.0.0",
        "vite": "4.4.6"
    },
    "peerDependencies": {
        "prettier": "3.0.0",
        "vite": "4.4.6"
    },
    "dependencies": {
        "chokidar": "3.5.3",
        "debug": "4.3.4",
        "fast-glob": "3.3.1"
    }
}
